definition
	EVALUATION[at0000] matches {	-- *Grunnlag to tph(nb)
		data matches {
			ITEM_TREE[at0001] matches {	-- *Tree(nb)
				items cardinality matches {3..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- *Begrunnelse for vedtaket er gitt i journalnotat av(nb)
						value matches {
							DV_DATE matches {
								value matches {yyyy-mm-dd}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- *Det gjøres unntak av konverteringsforbudet, jf. psykisk helsevernloven § 3-4 annet ledd(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- *Begrunnelse for unntaket er gitt i journalnotat av(nb)
						value matches {
							DV_DATE matches {
								value matches {yyyy-mm-dd}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- *Pasientens ankomst til institusjonen(nb)
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:MM:SS}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- *Det foreligger en begjæring om tvungent psykisk helsevern(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- *Dato for begjæring(nb)
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0008] matches {	-- *Begjæring fremmet av(nb)
						value matches {
							0|[local::at0009], 	-- *Kommunelegen eller dennes stedfortreder(nb)
							1|[local::at0010], 	-- *Den kommunale helse- og omsorgstjenesten(nb)
							2|[local::at0011], 	-- *Sosialtjenesten(nb)
							3|[local::at0012], 	-- *Politiet(nb)
							4|[local::at0013]  	-- *Ansvarlig fengselsmyndighet(nb)
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- *Begrunnelse for begjæring er gitt i journalnotat av(nb)
						value matches {
							DV_DATE matches {
								value matches {yyyy-mm-dd}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- *Pasienten er under tvungen observasjon(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0016] occurrences matches {0..1} matches {	-- *Oppstartdato for tvungen observasjon(nb)
						value matches {
							DV_DATE matches {
								value matches {yyyy-mm-dd}
							}
						}
					}
					ELEMENT[at0017] matches {	-- *Vurdering av frivillighet(nb)
						value matches {
							0|[local::at0018], 	-- *Frivillig psykisk helsevern er blitt forsøkt uten at dette har ført frem.(nb)
							1|[local::at0019]  	-- *Det er åpenbart formålsløst å forsøke frivillig psykisk helsevern(nb)
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- *Det anses som overveiende sannsynlig at pasienten har en alvorlig sinnslidelse(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {	-- *Tvungent psykisk helsevern er nødvendig for å hindre at vedkommende på grunn av sinnslidelsen(nb)
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0039] occurrences matches {0..1} matches {	-- *Tilleggsvilkår(nb)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0025] occurrences matches {0..1} matches {	-- *Får sin utsikt til helbredelse eller vesentlig bedring i betydelig grad redusert(nb)
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0027] occurrences matches {0..1} matches {	-- *Med stor sannsynlighet i meget nær fremtid får sin tilstand vesentlig forverret(nb)
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {	-- *Utgjør en nærliggende og alvorlig fare for eget liv og helse(nb)
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0029] occurrences matches {0..1} matches {	-- *Utgjør en nærliggende og alvorlig fare for andres liv og helse(nb)
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {	-- *Institusjonen er faglig og materielt i stand til å tilby pasienten tilfredsstillende behandling og omsorg og er godkjent i henhold til psykisk helsevernloven § 3-5(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- *Pasienten er gitt anledning til å uttale seg(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0032] occurrences matches {0..1} matches {	-- *Tvungent psykisk helsevern fremtrer etter en helhetsvurdering som den klart beste løsning for pasienten(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {	-- *Nærmeste pårørendes uttalerett, jf. psyksik helsevernloven § 3-9(nb)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0034] matches {	-- *New element(nb)
						value matches {
							0|[local::at0035], 	-- *Nærmeste pårørende er gitt anledning til å uttale seg(nb)
							1|[local::at0036], 	-- *Nærmeste pårørende er ikke gitt anledning til å uttale seg da dennes adresse er ukjent og ettersporing vil kreve urimelig mye tid eller arbeid(nb)
							2|[local::at0037]  	-- *Ingen personer med status som nærmeste pårørende(nb)
						}
					}
					ELEMENT[at0038] occurrences matches {0..1} matches {	-- *Den som har fremsatt begjæring etter psykisk helsevernloven § 3-6, er informert om vedtaket og sin klageadgang(nb)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
				}
			}
		}
	}

