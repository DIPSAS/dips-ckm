  <definition xsi:type="COMPOSITION" archetype_id="openEHR-EHR-COMPOSITION.encounter.v1" concept_name="Encounter" name="Vedtak om opphør av TPH">
    <Content xsi:type="ACTION" archetype_id="openEHR-EHR-ACTION.ny_formalia.v1" concept_name="Ny formalia" path="/content">
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.hjemmel.v1" concept_name="Hjemmel" path="/description[at0001]/items[at0002]">
        <Rule path="/items[at0001]">
          <constraint xsi:type="ordinalConstraint">
            <excludedValues>3| 3-2	Tvungen observasjon med døgnopphold. Psykisk helsevernloven  3-2 jf 3-5 første ledd</excludedValues>
