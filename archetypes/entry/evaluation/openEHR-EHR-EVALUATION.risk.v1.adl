definition
	EVALUATION[at0000] matches {	-- Helserisiko
		data matches {
			ITEM_TREE[at0001] matches {	-- structure(en)
				items cardinality matches {1..*; ordered} matches {
					ELEMENT[at0002] matches {	-- Risiko
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..*} matches {	-- Risikofaktor
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..*} matches {	-- Lenke til evidens/belegg
						value matches {
							DV_URI matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Kategori
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0006, 	-- Ikke signifikant
									at0007, 	-- Minimal signifikant
									at0008, 	-- Signifikant
									at0009]	-- Høy signifikant
								}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..*} matches {	-- Rasjonale
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

