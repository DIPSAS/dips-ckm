definition
	EVALUATION[at0000] matches {	-- tph
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {3..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Pasientens ankomst til institusjonen
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:??:??}
							}
						}
					}
					ELEMENT[at0016] occurrences matches {0..1} matches {	-- Oppstartdato for tvungen observasjon 
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					allow_archetype CLUSTER[at0068] occurrences matches {0..*} matches {	-- Hjemmel for helsehjelp
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0017] occurrences matches {0..2} matches {	-- Vurdering av frivillighet
						value matches {
							0|[local::at0018], 	-- Frivillig psykisk helsevern er blitt forsøkt uten at dette har ført frem.
							1|[local::at0019]  	-- Det er åpenbart formålsløst å forsøke frivillig psykisk helsevern
						}
					}
					ELEMENT[at0084] occurrences matches {0..*} matches {	-- Frivillighet er forsøkt eller er formålsløst, begrunnelse
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0040] occurrences matches {0..1} matches {	-- Ekstern legeundersøkelse
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0041] occurrences matches {0..1} matches {	-- Dato
								value matches {
									DV_DATE matches {
										value matches {yyyy-??-XX}
									}
								}
							}
							ELEMENT[at0070] occurrences matches {0..1} matches {	-- Pasienten har vært undersøkt av en ekstern lege
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0071] occurrences matches {0..1} matches {	-- Utført av
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[ac0003]}		-- New constraint
									}
								}
							}
							ELEMENT[at0077] occurrences matches {0..1} matches {	-- Utført av
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0043] occurrences matches {0..1} matches {	-- Ekstern legeundersøkelse dokumentert i journalnotat datert
								value matches {
									DV_DATE matches {
										value matches {yyyy-??-XX}
									}
								}
							}
							ELEMENT[at0044] occurrences matches {0..1} matches {	-- Legeundersøkelsen er foretatt på bakgrunn av vedtak om tvungen legeundersøkelse
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
						}
					}
					CLUSTER[at0045] occurrences matches {0..1} matches {	-- Intern legeundersøkelse
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0046] occurrences matches {0..1} matches {	-- Dato
								value matches {
									DV_DATE matches {
										value matches {yyyy-??-XX}
									}
								}
							}
							ELEMENT[at0047] occurrences matches {0..1} matches {	-- Utført av
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[ac0001]}		-- New constraint
									}
								}
							}
							ELEMENT[at0048] occurrences matches {0..1} matches {	-- Intern legeundersøkelse dokumentert i journalnotat datert
								value matches {
									DV_DATE matches {
										value matches {yyyy-??-XX}
									}
								}
							}
						}
					}
					ELEMENT[at0080] occurrences matches {0..*} matches {	-- Det vurderes at hovedvilkåret «alvorlig sinnslidelse» er oppfylt, begrunnelse 
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0062] occurrences matches {0..*} matches {	-- Tvungent psykisk helsevern er nødvendig for å hindre at vedkommende på grunn av sinnslidelsen
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0063, 	-- Får sin utsikt til helbredelse eller vesentlig bedring i betydelig grad redusert
									at0064, 	-- Med stor sannsynlighet i meget nær fremtid får sin tilstand vesentlig forverret
									at0065, 	-- Utgjør en nærliggende og alvorlig fare for eget liv og helse
									at0066]	-- Utgjør en nærliggende og alvorlig fare for andres liv og helse
								}
							}
						}
					}
					ELEMENT[at0081] occurrences matches {0..*} matches {	-- Tilleggsvilkår, begrunnelse 
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0082] occurrences matches {0..*} matches {	-- Institusjonen er faglig og materielt egnet for denne pasienten, begrunnelse 
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0083] occurrences matches {0..*} matches {	-- Helhetsvurdering, begrunnelse for at tvungent psykisk helsevern framstår som den klart beste løsningen for pasienten 
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..*} matches {	-- Det gjøres unntak fra konverteringsforbudet, jf. Psykisk helsevernloven § 3-4 annet ledd
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0074] occurrences matches {0..*} matches {	-- Begrunnelse for unntak fra konverteringsforbudet
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0005] occurrences matches {0..*} matches {	-- Det er framsatt begjæring (regelsett; info klageadgang)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0007] occurrences matches {0..*} matches {	-- Dato begjæringen ble fremmet
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					allow_archetype CLUSTER[at0072] occurrences matches {0..*} matches {	-- Begjæring framsatt av
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0014] occurrences matches {0..*} matches {	-- Begjæringen er dokumentert i journalnotat datert
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					ELEMENT[at0049] occurrences matches {0..*} matches {	-- Helsetjenesteenhet 
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0050] occurrences matches {0..*} matches {	-- Dato og tidspunkt vedtak fattet
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:??:??}
							}
						}
					}
					allow_archetype CLUSTER[at0069] occurrences matches {0..*} matches {	-- Vedtakstype (skal skjules)
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0076] occurrences matches {0..*} matches {	-- Vurdering av pasientinnsyn
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

