        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"This is a generic archetype used to record an ongoing and persistent summary of the use of any and all substances. ">
            keywords = <"consumption","substance","use","usage","dependence","tobacco","smoking","cigarettes","cigar","pipe">
            copyright = <"copyright (c) 2009 openEHR Foundation">
            use = <"Used to record an ongoing and persistent summary of any and all use of tobacco.  ">
            misuse = <"Not to be used for recording actual details about the use of the substance and event-related data - this should be captured using the OBSERVATION.substance_use-tobacco archetype.  
Not to be used to record information about any other substance other than tobacco.">
        >
        ["no"] = <
            language = <[ISO_639-1::no]>
        >
    >

definition
    EVALUATION[at0000.1] matches {
        data matches {
            ITEM_TREE[at0001] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[at0005] occurrences matches {0..1} matches {
                        value matches {
                            DV_CODED_TEXT matches {
                                defining_code matches {
                                    [local::at0.17]
                                }
                            }
                        }
                    }
                    ELEMENT[at0002] occurrences matches {0..1} matches {
                        value matches {
                            DV_CODED_TEXT matches {
                                defining_code matches {
                                    [local::
                                    at0003,
                                    at0004,
                                    at0006,
                                    at0016]
                                }
                            }
                        }
                    }
                    CLUSTER[at0007] occurrences matches {0..*} matches {
                        items cardinality matches {0..*; unordered} matches {
                            ELEMENT[at0008] occurrences matches {0..1} matches {
                                value matches {
                                    DV_CODED_TEXT matches {
                                        defining_code matches {
                                            [local::
                                            at0.18,
                                            at0.19,
                                            at0.20,
                                            at0.21]
                                        }
                                    }
                                }
                            }
                            ELEMENT[at0015] occurrences matches {0..1} matches {
                                value matches {
                                    DV_TEXT matches {*}
                                }
                            }
                            ELEMENT[at0009] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DATE_TIME matches {*}
                                }
                            }
                            ELEMENT[at0010] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DURATION matches {*}
                                }
                            }
                            ELEMENT[at0011] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DATE_TIME matches {*}
                                }
                            }
                            ELEMENT[at0012] occurrences matches {0..1} matches {
                                value matches {
                                    DV_DURATION matches {*}
                                }
                            }
                            ELEMENT[at0013] occurrences matches {0..1} matches {
                                value matches {
                                    DV_TEXT matches {*}
                                }
                            }
                        }
                    }
                    allow_archetype CLUSTER[at0014] occurrences matches {0..*} matches {
                        include
                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.cessation_attempts\.v1/}
                    }
                }
            }
        }
    }

