	>
	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Benytters for å registrere tidspunkt for planlagte hendelser i et pakkeforløp.">
			use = <"Utviklet for registrering av tidspunkt for planlagte hendelser i et pakkeforløp.">
			misuse = <"">
			copyright = <"">
		>
	>
	lifecycle_state = <"0">
