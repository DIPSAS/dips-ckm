definition
	ACTION[at0000] matches {	-- LegemiddelhandlingDIPS
		ism_transition matches {
			ISM_TRANSITION[at0043] matches {	-- Satt ut av bruk
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::532]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0043]}		-- Satt ut av bruk
					}
				}
			}
			ISM_TRANSITION[at0044] matches {	-- Tilbakekalt
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::532]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0044]}		-- Tilbakekalt
					}
				}
			}
			ISM_TRANSITION[at0045] matches {	-- Satt ut av bruk og tilbakekalt
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::532]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0045]}		-- Satt ut av bruk og tilbakekalt
					}
				}
			}
		}
		description matches {
			ITEM_TREE[at0017] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0020] matches {	-- Legemiddel
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {[ac0001]}		-- Legemiddelnavn
							}
						}
					}
					ELEMENT[at0033] occurrences matches {0..*} matches {	-- Instruksjoner
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0023] occurrences matches {0..*} matches {	-- Ingredienser og form
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication_ingredients(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0021] occurrences matches {0..*} matches {	-- Årsak
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0022] occurrences matches {0..1} matches {	-- Kvantitet
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication_amount(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {	-- Kommentarer
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- Sekvensnummer
						value matches {
							DV_COUNT matches {*}
						}
					}
					allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {	-- Administrasjon
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication_admin(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0036] occurrences matches {0..1} matches {	-- Generisk bytte
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0030] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0026] occurrences matches {0..1} matches {	-- Batch ID
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0040] occurrences matches {0..1} matches {	-- Utløpsdato
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Utdelt til
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0037] occurrences matches {0..1} matches {	-- Antall utleveringer
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0032] occurrences matches {0..1} matches {	-- Gjenværende utleveringer
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0038] occurrences matches {0..1} matches {	-- Refusjon
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["no"] = <
			items = <
				["at0000"] = <
					text = <"Legemiddelhandling">
					description = <"Detaljer om bruk, administrasjon, utdeling eller andre tiltak i forbindelse med legemiddel, vaksine eller annet terapeutisk forbruksmateriell ordinert av en kliniker.">
				>
				["at0017"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0020"] = <
					text = <"Legemiddel">
					description = <"Legemiddelet, vaksinen eller det terapeutiske forbruksmaterialet som er fokus for ordinasjonen. Dette elementet vil normalt bli kodet med en legemiddelterminologi, men fritekst kan kreves i særlige situasjoner.">
				>
				["at0021"] = <
					text = <"Årsak">
					description = <"Årsaken(e) til den spesifikke handlingen. NB; Dette er ikke indikasjonen for medisinering, men årsak til endringer eller seponering av medisineringen.">
				>
				["at0022"] = <
					text = <"Kvantitet">
					description = <"Mengde av legemiddel, vaksine eller annet terapeutisk forbruksmateriell.">
				>
				["at0023"] = <
					text = <"Ingredienser og form">
					description = <"Detaljert informasjon om ingredienser inkludert form og styrke.">
				>
				["at0024"] = <
					text = <"Kommentarer">
					description = <"Kommentar til handlingen.">
				>
				["at0025"] = <
					text = <"Sekvensnummer">
					description = <"Sekvensnummer som er spesifikt til denne handlingen.">
				>
				["at0026"] = <
					text = <"Batch ID">
					description = <"Tildelt av produsenten for å identifisere produksjonsbatch av produktet.">
				>
				["at0027"] = <
					text = <"Administrasjon">
					description = <"Detaljer om administreringen av legemiddel, vaksinasjon eller annet terapeutisk forbruksmateriell.">
				>
				["at0030"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0031"] = <
					text = <"Utdelt til">
					description = <"Navnet til personen som henter medisinen, om dette ikke er pasienten selv.">
				>
				["at0032"] = <
					text = <"Gjenværende utleveringer">
					description = <"Antall ganger legemiddelet, vaksinen eller det terapeutiske forbruksmateriellet kan utleveres uten fornying av resept eller ordinasjon. Merk: Dette er informasjon som kreves av pasienten og resept utsteder.">
				>
				["at0033"] = <
					text = <"Instruksjoner">
					description = <"Instruksjoner gitt til en pasient eller helsearbeider ved oppstart.">
				>
				["at0036"] = <
					text = <"Generisk bytte">
					description = <"En variant av samme legemiddel , vaksine eller terapeutisk forbruksmateriell fra annen produsent erstatter det ordinerte.">
				>
				["at0037"] = <
					text = <"Antall utleveringer">
