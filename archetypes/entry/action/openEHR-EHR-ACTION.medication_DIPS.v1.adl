				>
				["at0038"] = <
					text = <"Refusjon">
					description = <"Refusjonskategori eller tilskudd som ønskes for medisinen.">
				>
				["at0040"] = <
					text = <"Utløpsdato">
					description = <"Utløpsdato som spesifisert av produsenten.">
