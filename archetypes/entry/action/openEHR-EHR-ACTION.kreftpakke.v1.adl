						defining_code matches {[local::at0029]}		-- Avslutning pakkeforløp
					}
				}
			}
			ISM_TRANSITION[at0026] matches {	-- Behandling start
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::532]}
					}
				}
