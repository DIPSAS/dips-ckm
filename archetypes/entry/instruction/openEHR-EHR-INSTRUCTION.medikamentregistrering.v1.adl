							}
							CLUSTER[at0026] occurrences matches {0..1} matches {	-- Utlevering
								items cardinality matches {1..*; unordered} matches {
									allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {	-- Mengde
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication_amount(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0028] occurrences matches {0..1} matches {	-- Antall gjentakelser/Reit
