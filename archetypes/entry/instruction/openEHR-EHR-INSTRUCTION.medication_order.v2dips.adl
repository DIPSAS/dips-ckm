										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {	-- Opphørs dato
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {	-- Varighet av behandling
										value matches {
											DV_DURATION matches {*}
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {	-- Varigheten av den tidligere behandling
