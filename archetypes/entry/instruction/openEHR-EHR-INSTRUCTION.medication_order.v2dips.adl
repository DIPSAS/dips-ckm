									}
								}
							}
							ELEMENT[at0035] occurrences matches {0..*} matches {	-- Kommentar
								value matches {
									DV_TEXT matches {*}
								}
							}
							CLUSTER[at0023] occurrences matches {0..1} matches {	-- Utlevering
								items cardinality matches {1..*; unordered} matches {
									allow_archetype CLUSTER[at0045] occurrences matches {0..*} matches {	-- Mengde
