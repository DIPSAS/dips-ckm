	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Å dokumentere anestesipersonellets evaluering av undersøkelser som er gjort og pasientens tilstand i forkant av en planlagt anestesi.">
			use = <"Brukes i forbindelse med anestesivurdering når en anestesi er planlagt, enten alene eller i forbindelse med en kirurgisk prosedyre. Brukes for å dokumentere anestesipersonellets vurdering av pasientens tilstand og evt. undersøkelser/diagnostikk.">
			keywords = <"anestesi", "risiko">
			misuse = <"">
			copyright = <"">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
