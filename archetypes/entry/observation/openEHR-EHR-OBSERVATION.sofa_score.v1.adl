	term_definitions = <
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Sofa score">
					description = <"Sequential (sepsis related) organ failure assessment (SOFA) er et klinisk verktøy for å kategorisere organdysfunksjon.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"*">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"PaO2/FiO2">
					description = <"Evaluering av respirasjon.">
				>
				["at0005"] = <
					text = <"> 53,3 kPa">
					description = <"Partialtrykket av innåndet oksygen målt i kPa dividert med fraksjonen av innåndet oksygen er større enn 53,3 kPa.">
				>
				["at0006"] = <
					text = <"< 53,3 kPa">
					description = <"Partialtrykket av innåndet oksygen målt i kPa dividert med fraksjonen av innåndet oksygen er mindre enn 53,3 kPa.">
				>
				["at0007"] = <
					text = <"< 40 kPa">
					description = <"Partialtrykket av innåndet oksygen målt i kPa dividert med fraksjonen av innåndet oksygen er mindre enn 40 kPa.">
				>
				["at0008"] = <
					text = <"< 26,7 kPa">
					description = <"Med ventilasjonsstøtte er partialtrykket av innåndet oksygen målt i kPa dividert med fraksjonen av innåndet oksygen mindre enn 26,7 kPa.">
				>
				["at0009"] = <
					text = <"< 13,3 kPa">
