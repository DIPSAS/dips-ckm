definition
	OBSERVATION[at0000] matches {	-- Sofa score
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						offset matches {
							DV_DURATION matches {
								value matches {|PT1H|}
							}
						}
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {12..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {12} matches {	-- PaO2/FiO2
										value matches {
											0|[local::at0005], 	-- > 53,3 kPa
											1|[local::at0006], 	-- < 53,3 kPa
											2|[local::at0007], 	-- < 40 kPa
											3|[local::at0008], 	-- < 26,7 kPa
											4|[local::at0009]  	-- < 13,3 kPa
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Sirkulasjon
										value matches {
											0|[local::at0011], 	-- MAP > 70 mmHg
											1|[local::at0012], 	-- MAP < 70 mmHg
											2|[local::at0013], 	-- DA < 5; DB
											3|[local::at0014], 	-- DA 5,1-15; A ≤ 0,10; N ≥ 0,10
											4|[local::at0015]  	-- DA > 15; A > 0,10; N > 0,10
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- Glasgow Coma Score
										value matches {
											0|[local::at0017], 	-- GCS 15
											1|[local::at0018], 	-- GCS 13-14
											2|[local::at0019], 	-- GCS 10-12
											3|[local::at0020], 	-- GCS 6-9
											4|[local::at0021]  	-- GCS < 6
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {	-- Kreatinin og døgnurin
										value matches {
											0|[local::at0023], 	-- < 110 µmol/l kreatinin
											1|[local::at0024], 	-- 110-170 µmol/l kreatinin
											2|[local::at0025], 	-- 171-299 µmol/l kreatinin
											3|[local::at0026], 	-- 300-440 µmol/l kreatinin eller døgnurin < 500
											4|[local::at0027]  	-- > 440 µmol/l kreatinin eller døgnurin < 200
										}
									}
									ELEMENT[at0028] occurrences matches {0..1} matches {	-- Bilirubin
										value matches {
											0|[local::at0029], 	-- < 20 µmol/l
											1|[local::at0030], 	-- 20-32 µmol/l
											2|[local::at0031], 	-- 33-101 µmol/l
											3|[local::at0032], 	-- 102-204 µmol/l
											4|[local::at0033]  	-- > 204 µmol/l
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {	-- Trombocyttall
										value matches {
											0|[local::at0035], 	-- ≥ 150 (x10³/µl)
											1|[local::at0036], 	-- < 150 (x10³/µl)
											2|[local::at0037], 	-- < 100 (x10³/µl)
											3|[local::at0038], 	-- < 50 (x10³/µl)
											4|[local::at0039]  	-- < 20 (x10³/µl)
										}
									}
									ELEMENT[at0041] occurrences matches {0..1} matches {	-- SOFA score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..28|}
											}
										}
									}
									ELEMENT[at0044] occurrences matches {0..1} matches {	-- Kommentar
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

