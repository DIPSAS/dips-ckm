													411, 
													412, 
													413, 
													517, 
													518, 
													519, 
													637, 
													682, 
													683, 
													401, 
													402, 
													404, 
													405, 
													406, 
													407, 
													414, 
													415, 
													416, 
													417, 
													418, 
													419, 
													420, 
													421, 
													422, 
													423, 
													424, 
													425, 
													426, 
													427, 
													428, 
													429]
												}
											}
										}
									}
									ELEMENT[at0033] occurrences matches {0..1} matches {	-- URI
										value matches {
											DV_URI matches {*}
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {	-- Proportion Allow all As Percent no limits
										value matches {
											DV_PROPORTION matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

