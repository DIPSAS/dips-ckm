definition
	OBSERVATION[at0000] matches {	-- Pim3 score
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Systolisk blodtrykk (SBT)
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														magnitude = <|>=0.0|>
														precision = <|0|>
													>
												>
											>
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Pupillereaksjon på skarpt lys
										value matches {
											0|[local::at0006], 	-- Annen reaksjon eller ukjent
											1|[local::at0007]  	-- ≥ 3 mm og begge fikserte
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- PaO2
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														magnitude = <|>0.0|>
														precision = <|0|>
													>
												>
											>
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- FiO2
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::507]>
												list = <
													["1"] = <
														units = <"">
														magnitude = <|>0.0|>
														precision = <|2|>
													>
												>
											>
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Base excess i arterielt eller kapillært blod
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"mmol/l">
														magnitude = <|-30.0..30.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- Mekanisk ventilasjon på noe tidspunkt i løpet av første timen på intensivenheten?
										value matches {
											0|[local::at0012], 	-- Nei
											1|[local::at0013]; 	-- Ja
											0  	-- assumed value
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {	-- Elektiv innleggelse i intensivenheten?
										value matches {
											0|[local::at0015], 	-- Nei
											1|[local::at0016]  	-- Ja
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Overvåkning/oppvåkning etter kirurgi eller en prosedyre er hovedgrunn for innleggelse i intensivenheten
										value matches {
											0|[local::at0021], 	-- Nei
											1|[local::at0022], 	-- Ja, overvåkning/oppvåkning etter en ikke hjerterelatert prosedyre
											2|[local::at0023], 	-- Ja, overvåkning/oppvåkning etter en hjerteprosedyre uten hjerte-lungemaskin
											3|[local::at0024]  	-- Ja, overvåkning/oppvåkning etter en hjerteprosedyre med hjerte-lungemaskin
										}
									}
									CLUSTER[at0043] occurrences matches {0..1} matches {	-- Vektet diagnose
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0034] occurrences matches {0..1} matches {	-- Lav risiko
												value matches {
													0|[local::at0035], 	-- Nei eller usikker
													1|[local::at0036]  	-- Ja
												}
											}
											ELEMENT[at0037] occurrences matches {0..1} matches {	-- Høy risiko
												value matches {
													0|[local::at0038], 	-- Nei eller usikker
													1|[local::at0039]  	-- Ja
												}
											}
											ELEMENT[at0040] occurrences matches {0..1} matches {	-- Svært høy risiko
												value matches {
													0|[local::at0041], 	-- Nei eller usikker
													1|[local::at0042]  	-- Ja
												}
											}
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {	-- PIM3 score
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"1">
														magnitude = <|>=0.0|>
														precision = <|3|>
													>
												>
											>
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {	-- Sannsynlighet for død
										value matches {
											DV_PROPORTION matches {
												numerator matches {|>=0.0|}
												is_integral matches {False}
												type matches {0}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0044] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0045] occurrences matches {0..1} matches {	-- PIM3 score
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::507]>
								list = <
									["1"] = <
										units = <"">
										magnitude = <|>=0.0|>
										precision = <|3|>
									>
								>
							>
						}
					}
					ELEMENT[at0046] occurrences matches {0..1} matches {	-- Sannsynlighet for død
						value matches {
							DV_PROPORTION matches {
								numerator matches {|>=0.0|}
								is_integral matches {False}
							}
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Pim3 score">
					description = <"Pediatric index of mortality (PIM) 3 score er et skåringsverktøy brukt å predikere utfallet for barn innlagt på en intensivenhet.">
				>
				["at0001"] = <
					text = <"Event Series">
