			purpose = <"Brukes til registrering av pasient- og pasientadministrativ informasjon som skal rapporteres til Norsk intensivregister (NIR).">
			use = <"Rappportering til Norsk intensivregister gjennomføres av intensivavdelinger i Norge. Arektypen benyttes til å registrere NIR-spesifikk informasjon.">
			misuse = <"">
			copyright = <"">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
