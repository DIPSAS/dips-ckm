					description = <"Konsentrasjonen av bilirubin er mellom 102 og 204 mikromol per liter.">
				>
				["at0034"] = <
					text = <"TPK ≥ 150 (x10³/µl)">
					description = <"TPK er større eller lik 150 000 per mikroliter.">
				>
				["at0035"] = <
