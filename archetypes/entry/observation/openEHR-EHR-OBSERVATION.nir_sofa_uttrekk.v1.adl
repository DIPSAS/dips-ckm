definition
	OBSERVATION[at0000] matches {	-- Nir sofa uttrekk
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Respirasjon
										value matches {
											0|[local::at0011], 	-- PaO2/FiO2 > 53 kPa
											1|[local::at0012], 	-- PaO2/FiO2 40 - 53 kPa
											2|[local::at0013], 	-- PaO2/FiO2 < 40 kPa
											3|[local::at0014], 	-- PaO2/FiO2 13,3 - 26,6 kPa
											4|[local::at0039]  	-- PaO2/FiO2 < 13,3 kPa
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Sirkulasjon
										value matches {
											0|[local::at0015], 	-- MAP ≥ 70 mmHg
											1|[local::at0016], 	-- MAP < 70 mmHg
											2|[local::at0017], 	-- DA < 5 µg/kg/m; Dobutamin
											3|[local::at0018], 	-- DA ≥ 5 µg/kg/m; NA/A < 0,1µg/kg/m
											4|[local::at0019]  	-- NA/A ≥ 0,1 µg/kg/m ; VP
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- SNS (sentralnervesystemet)
										value matches {
											0|[local::at0020], 	-- GCS 14-15
											1|[local::at0021], 	-- GCS 12-13
											2|[local::at0022], 	-- GCS 9-11
											3|[local::at0023], 	-- GCS 6-8
											4|[local::at0024]  	-- GCS 3-5
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Nyre
										value matches {
											0|[local::at0025], 	-- Kreatinin < 110 µmol/l
											1|[local::at0026], 	-- Kreatinin≥ 110 µmol/l
											2|[local::at0027], 	-- Kreatinin ≥ 171 µmol/l
											3|[local::at0028], 	-- Kreatinin ≥ 300 µmol/l eller DU < 500 ml
											4|[local::at0029]  	-- Kreatinin ≥ 440 µmol/l eller DU < 200 ml eller nyreerstattende behandling: HD, HF, PD
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Lever
										value matches {
											0|[local::at0030], 	-- Bilirubin < 20 µmol/l
											1|[local::at0031], 	-- Bilirubin 20-32 µmol/l
											2|[local::at0032], 	-- Bilirubin 33-101 µmol/l
											3|[local::at0033], 	-- Bilirubin 102-204 µmol/l
											4|[local::at0040]  	-- Bilirubin > 204 µmol/l
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Koagulasjon
										value matches {
											0|[local::at0034], 	-- TPK ≥ 150 (x10³/µl)
											1|[local::at0035], 	-- TPK < 150 (x10³/µl)
											2|[local::at0036], 	-- TPK < 100 (x10³/µl)
											3|[local::at0037], 	-- TPK < 50 (x10³/µl)
											4|[local::at0038]  	-- TPK < 20 (x10³/µl)
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Maksimal total SOFA score
										value matches {
