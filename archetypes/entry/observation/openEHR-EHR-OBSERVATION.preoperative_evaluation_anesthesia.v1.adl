concept
	[at0000]	-- Preoperativ evaluering anestesi
language
	original_language = <[ISO_639-1::nb]>
description
	original_author = <
		["name"] = <"Anca Heyd">
		["organisation"] = <"DIPS ASA">
		["email"] = <"ahe@dips.no">
		["date"] = <"2015-03-01">
	>
	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Å dokumentere anestesipersonellets evaluering av undersøkelser som er gjort og pasientens tilstand i forkant av en planlagt anestesi.">
			use = <"Brukes i forbindelse med anestesivurdering når en anestesi er planlagt, enten alene erller i forbindelse med en irurgisk prosedyre. Brukes for å dokumentere anestesilegens vurdering av pasientens tilstand og evt. undersøkelser/diagnostikk.">
			keywords = <"anestesi", "risiko">
			misuse = <"">
			copyright = <"">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
	other_details = <
		["current_contact"] = <"Anca Heyd, DIPS ASA, ahe@dips.no">
		["MD5-CAM-1.0.1"] = <"873C1A0CD9055032AB5B59F98D8718B0">
	>

