  <ontology>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Kritisk info classic</items>
        <items id="description">Midlertidig arketype for kritisk info til bruk i skjema som PLO-skjema, epikrise, etc. Arketypen baserer seg på kritisk info registrert i Classic.  </items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">*</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Type</items>
        <items id="description">Beskriver type kritisk informasjon, f.eks. smitte</items>
      </items>
      <items code="at0005">
        <items id="text">Klassifisering</items>
        <items id="description">Klassifisering av type kritisk informasjon. Kunne legge inn mer utfyllende beskrivelse, f.eks blodsmitte.</items>
      </items>
      <items code="at0006">
        <items id="text">Alvorlighetsgrad</items>
        <items id="description">Beskriver alvorlighetsgrad av kritisk informasjon, f.eks. viktig.</items>
      </items>
      <items code="at0007">
        <items id="text">Isolasjonstype</items>
        <items id="description">Beskrivelse av smitterisiko, f.eks. kontaktsmitte</items>
      </items>
      <items code="at0008">
        <items id="text">Beskrivelse</items>
        <items id="description">Felt for å legge inn ytterligere beskrivelse, f.eks. hepatitt C</items>
      </items>
      <items code="at0010">
        <items id="text">Startdato</items>
        <items id="description">Dato for start kritisk info</items>
      </items>
      <items code="at0011">
        <items id="text">Sluttdato</items>
        <items id="description">Dato for slutt kritisk info. Eks kontaktsmitte som kan opphøre.</items>
      </items>
      <items code="at0012">
        <items id="text">Sendes</items>
        <items id="description">Hvis kritisk informasjon skal sendes ut.</items>
      </items>
    </term_definitions>
