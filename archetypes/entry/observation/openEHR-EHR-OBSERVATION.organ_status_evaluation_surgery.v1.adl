definition
	OBSERVATION[at0000] matches {	-- Organstatus evaluering kirurgi
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- Hjertefunksjon
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {	-- Status
												value matches {
													1|[local::at0026], 	-- Test
													2|[local::at0027]  	-- Annen test
												}
											}
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- Kommentar
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0007] occurrences matches {0..1} matches {	-- Lungefunksjon
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- Status
												value matches {
													C_DV_ORDINAL <
													>
												}
											}
											ELEMENT[at0009] occurrences matches {0..1} matches {	-- Kommentar
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0010] occurrences matches {0..1} matches {	-- Nyrefunksjon
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0011] occurrences matches {0..1} matches {	-- Status
												value matches {
													C_DV_ORDINAL <
													>
												}
											}
											ELEMENT[at0012] occurrences matches {0..1} matches {	-- Kommentar
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0013] occurrences matches {0..1} matches {	-- Mentalfunksjon
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0014] occurrences matches {0..1} matches {	-- Status
												value matches {
													C_DV_ORDINAL <
													>
												}
											}
											ELEMENT[at0015] occurrences matches {0..1} matches {	-- Beskrivelse
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- Økt blødningsrisiko
										value matches {
											DV_BOOLEAN matches {
												value matches {True}
											}
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Annet
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0023] occurrences matches {0..*} matches {	-- ASA fysisk status klassifkasjon 
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.asa_status(-[a-zA-Z0-9_]+)*\.v1/}
									}
								}
							}
						}
					}
				}
			}
		}
	}

