			language = <[ISO_639-1::nb]>
			purpose = <"Å gi en beskrivelse av risikofaktorer ved hjertet som har betydning for en planlagt anestesi.">
			use = <"Brukes i forbindelse med planlegging av en prosedyre som trenger anestesi eller i forkant av en anestesi for å vurdere risikofaktorer ved hjertet som kan ha betydning betydning for anestesien.">
			keywords = <"hjerte", "organ", "risikofaktor", "anestesi">
			misuse = <"Brukes ikke for å beskrive undersøkelsesfunn ved hjerte. Til dette brukes f.eks. arketypen funn ved klinisk undersøkelse. Brukes ikke for å registrere en diagnose ved hjertesykdom.">
