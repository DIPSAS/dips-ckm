definition
	ADMIN_ENTRY[at0000] matches {	-- Klage pa vedtak etter psykisk helsevernloven
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Klagen gjelder vedtak i journaldokument av
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					ELEMENT[at0038] occurrences matches {0..1} matches {	-- Vedtakstype
						value matches {
							0|[local::at0039], 	-- Vedtak om tvungen observasjon
							1|[local::at0040], 	-- Vedtak om opphør av tvungen observasjon
							2|[local::at0041], 	-- Vedtak om å ikke etablere tvungen observasjon/tvungent psykisk helsevern
							3|[local::at0042], 	-- Vedtak om tvungent psykisk helsevern
							4|[local::at0043], 	-- Vedtak om opphør av tvungent psykisk helsevern
							5|[local::at0044], 	-- Vedtak om overføring til annen institusjon
							6|[local::at0045], 	-- Vedtak om overføring til annen tvangsform
							7|[local::at0046], 	-- Vedtak om overføring av domfelt til annen institusjon
							8|[local::at0047], 	-- Vedtak om overføring av domfelt til annen tvangsform
							9|[local::at0048], 	-- Vedtak om skjerming
							10|[local::at0049], 	-- Vedtak om bruk av mekaniske tvangsmidler
							11|[local::at0050], 	-- Vedtak om behandling uten eget samtykke
							12|[local::at0051], 	-- Vedtak om innskrenket forbindelse med omverdenen
							13|[local::at0052], 	-- Vedtak om åpning og kontroll av post
							14|[local::at0053], 	-- Vedtak om undersøkelse av rom og eiendeler og/eller kroppsvisitasjon
							15|[local::at0054], 	-- Vedtak om beslag og eventuell tilintetgjørelse
							16|[local::at0055], 	-- Vedtak om rusmiddeltesting av biologisk materiale
							17|[local::at0056], 	-- Vedtak om undersøkelse av pasientens person, rom og eiendeler
							18|[local::at0057], 	-- Vedtak om kroppslig undersøkelse
							19|[local::at0058], 	-- Vedtak om innskrenkninger i pasientens forbindelse med omverdenen
							20|[local::at0059]  	-- Vedtak om nattelåsing av pasientrom
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Framsatt tidspunkt
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:??:??}
							}
						}
					}
					ELEMENT[at0027] occurrences matches {0..1} matches {	-- Klage framsatt av
						value matches {
							0|[local::at0028], 	-- Pasienten selv
							1|[local::at0029], 	-- Nærmeste pårørende
							2|[local::at0030]  	-- Den offentlige myndighet som har fremsatt begjæring om tvungen observasjon eller tvungent psykisk helsevern
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- Klageinstans
						value matches {
							0|[local::at0015], 	-- Fylkesmannen
							1|[local::at0016]  	-- Kontrollkommisjonen
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Status for klagen
						value matches {
							0|[local::at0032], 	-- Klage framsatt men ikke avgjort
							1|[local::at0033], 	-- Klagen er trukket
							2|[local::at0034], 	-- Klagen er avvist
							3|[local::at0035], 	-- Kontrollkommisjonens leder har besluttet utsatt iverksetting av vedtak inntil klagesaken er avgjort
							4|[local::at0036], 	-- Klager er ikke gitt medhold
							5|[local::at0037]  	-- Klager er gitt medhold
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {	-- Resultat dato
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Klage framsatt av (utgår)
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0025, 	-- Pasienten selv
									at0006, 	-- Nærmeste pårørende
									at0007]	-- Den offentlige myndighet som har fremsatt begjæring om begjæring om tvungen observasjon eller tvungent psykisk helsevern
								}
							}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Status for klagen (utgår)
						value matches {
							0|[local::at0017], 	-- Klage framsatt men ikke avgjort
							1|[local::at0018], 	-- Klagen er trukket
							2|[local::at0019], 	-- Klagen er avvist
							3|[local::at0020], 	-- Kontrollkommisjonens leder har besluttet utsatt iverksetting av vedtak inntil klagesaken er avgjort
							4|[local::at0021], 	-- Klager er gitt medhold
							5|[local::at0022]  	-- Klager er ikke gitt medhold
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {	-- Status (utgår)
						value matches {
							C_DV_ORDINAL <
							>
						}
					}
				}
			}
		}
	}

