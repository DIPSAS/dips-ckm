					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- NIR oppholdstid i intensivenheten i timer
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- NIR oppholdstid i intensivenheten i døgn
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::128]>
								list = <
									["1"] = <
										units = <"d">
										magnitude = <|>=0.0|>
										precision = <|1|>
									>
								>
							>
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- NIR varighet av non-invasiv mekanisk ventilasjon
