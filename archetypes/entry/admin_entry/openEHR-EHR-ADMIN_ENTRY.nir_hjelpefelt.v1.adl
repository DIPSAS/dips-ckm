							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- NIR alder ved innleggelse i intensivenheten
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
