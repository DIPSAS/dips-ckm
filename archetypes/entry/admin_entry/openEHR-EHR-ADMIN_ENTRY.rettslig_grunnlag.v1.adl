definition
	ADMIN_ENTRY[at0000] matches {	-- Rettslig grunnlag
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0026] occurrences matches {0..1} matches {	-- Hjemmel for helsehjelp
						value matches {
							0|[local::at0102], 	-- Frivillig, eget samtykke. Psykisk helsevernloven § 2-1
							1|[local::at0103], 	-- Frivillig, representert samtykke. Psykisk helsevernloven § 2-1, jf. pasient- og brukerrettighetsloven kapittel 4
							2|[local::at0104], 	-- Samtykke til å være undergitt reglene om tvungent psykisk helsevern i § 3-5. Psykisk helsevernloven § 2-2
							3|[local::at0105], 	-- Tvungen observasjon. Psykisk helsevernloven § 3-2
							4|[local::at0106], 	-- Tvungent psykisk helsevern. Psykisk helsevernloven § 3-3
							5|[local::at0107], 	-- Tvungent psykisk helsevern uten døgnopphold med frivillig innleggelse. Psykisk helsevernloven § 3-5 tredje ledd, jf. § 2-1
							6|[local::at0108], 	-- Dom på overføring til tvungent psykisk helsevern. Straffeloven § 62 første ledd første punktum, jf. psykisk helsevernloven kapittel 5
							7|[local::at0109], 	-- Dom på overføring til tvungent psykisk helsevern. Straffeloven § 62 første ledd andre punktum, jf. psykisk helsevernloven kapittel 5
							8|[local::at0110], 	-- Dom på tvungen omsorg. Straffeloven § 63
							9|[local::at0111], 	-- Rettsbestemt innlegges til undersøkelse på psykiatrisk sykehus eller annet egnet undersøkelsessted. Straffeprosessloven § 167
							10|[local::at0112], 	-- Rettsbestemt at varetekt skal skje med plassering i psykisk helsevern. Straffeprosessloven § 188
							11|[local::at0113], 	-- Barnevernloven
							12|[local::at0114], 	-- Tilbakehold av rusmiddelavhengig i institusjon på grunnlag av eget samtykke, jf. helse- og omsorgstjenesteloven § 10-4 første ledd
							13|[local::at0115], 	-- Tilbakehold av rusmiddelavhengig i institusjon på bakgrunn av eget samtykke, jf. helse- og omsorgstjenesteloven § 10-4 andre ledd
							14|[local::at0116], 	-- Fylkesnemdsvedtak om tilbakehold av rusmiddelavhengig i institusjon uten eget samtykke. Helse- og omsorgstjenesteloven § 10-2 første ledd
							15|[local::at0117], 	-- Midlertidig vedtak av kommunen om tilbakehold av rusmiddelavhengig. Helse- og omsorgstjenesteloven § 10-2 fjerde ledd
							16|[local::at0118], 	-- Fylkesnemdsvedtak om tilbakeholdelse av gravide rusmiddelavhengige. Helse- og omsorgstjenesteloven § 10-3 første ledd
							17|[local::at0119], 	-- Midlertidig vedtak av kommunen om tilbakeholdelse av gravide rusmiddelavhengige. Helse- og omsorgstjenesteloven § 10-3 femte ledd
							18|[local::at0120], 	-- Nødrett (Straffeloven § 17)
							19|[local::at0121]  	-- Annen lovhjemmel/annet grunnlag
						}
					}
					ELEMENT[at0027] occurrences matches {0..1} matches {	-- Annen lovhjemmel eller annet grunnlag 
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0028] occurrences matches {0..1} matches {	-- Med eller uten døgnopphold
						value matches {
							0|[local::at0122], 	-- Behandling med døgnopphold
							1|[local::at0123]  	-- Behandling uten døgnopphold
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Pasienten har opphold på institusjon som ikke er godkjent for tvungent psykisk helsevern
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Dato påbegynt
						value matches {
							DV_DATE matches {
								value matches {yyyy-mm-dd}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Dato tilbakeført godkjent institusjon
						value matches {
							DV_DATE matches {
								value matches {yyyy-mm-dd}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Vilkår for utsettelse av straff, jf. straffeloven § 53 nr 3f
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Utsettelse
						value matches {
							0|[local::at0124], 	-- Psykiatrisk behandling som vilkår for utsettelse av straff
							1|[local::at0125]  	-- Psykiatrisk behandling på institusjon som vilkår for utsettelse av straff
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Godskrives som gjennomføring av straff, jf. straffegjennomføringsloven
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {	-- Gjennomføring
						value matches {
							0|[local::at0126], 	-- Gjennomføring av straff i institusjon (Straffegjennomføringsloven § 12)
							1|[local::at0127]  	-- Gjennomføring av straff i sykehus (Straffegjennomføringsloven § 13)
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- Startdato og tidspunkt aktuelt rettslig grunnlag
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:??:??}
							}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Aktuelt rettslig grunnlag gyldig til
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:??:??}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Forlengelse nr.
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- Status rettslig grunnlag
						value matches {
							0|[local::at0128], 	-- Aktuelt rettslig grunnlag for behandlingen
							1|[local::at0129], 	-- Gyldighetsperiode for rettslig grunnlag overskredet uten at nytt rettslig grunnlag er etablert
							2|[local::at0130], 	-- Feilregistrering, erstattet av nytt, korrekt rettslig grunnlag
							3|[local::at0131], 	-- Erstattet av et oppdatert rettslig grunnlag etter forlengelse av gyldighetsperioden
							4|[local::at0132], 	-- Erstattet av nytt rettslig grunnlag i henhold til nytt vedtak/dom
							5|[local::at0133], 	-- Pasienten overført til annen institusjon innenfor det psykiske helsevern. (tvang opprettholdt)
							6|[local::at0134], 	-- Tvang opphørt, pasienten overført til annen institusjon innenfor det psykiske helsevern
							7|[local::at0135], 	-- Tvang opphørt, pasienten fortsatt ved institusjonen (nytt rettslig grunnlag registrert)
							8|[local::at0136]  	-- Avsluttet, pasienten utskrevet
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Helsetjenesteenhet
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {	-- Merknad
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

