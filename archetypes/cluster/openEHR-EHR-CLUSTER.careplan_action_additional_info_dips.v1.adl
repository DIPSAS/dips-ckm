definition
	CLUSTER[at0000] matches {	-- Details related action performed
		items cardinality matches {1; unordered} matches {
			ELEMENT[at0003] occurrences matches {0..1} matches {	-- Record as not performed
				value matches {
					DV_BOOLEAN matches {
						value matches {True, False}
					}
				}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {	-- Link to clinical note
				value matches {
					DV_EHR_URI matches {*}
				}
			}
			ELEMENT[at0006] occurrences matches {0..1} matches {	-- *Registrer som påbegynt(nb)
				value matches {
					DV_BOOLEAN matches {
						value matches {True, False}
					}
				}
			}
		}
	}

